
<div id="find">
  <form>
    <label for="query">Query: </label>
    {{ input type="text" id="query" value=search }}
  </form>
</div>


<ul>
  {{#each spot in arrangedContent}}
    <li class="spot">
      {{link-to spot.name 'spots.show' spot}}
    </li>
  {{/each}}
</ul>
<div id="new-spot-link">
  {{#link-to 'spots.new'}}Add a spot{{/link-to}}
</div>
<iframe
width="500"
height="325"
frameborder="0" style="border:0"
src="https://www.google.com/maps/embed/v1/search?key=AIzaSyAs1EqVwjjQtzplL4v02060BCBctNNN6Ww
&q=thrift+stores+in+Portland">
</iframe>
